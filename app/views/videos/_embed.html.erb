<% if (seek||=0) > 0 %>
  <iframe id="player"
          type="text/html"
          width="640"
          height="390"
          src="https://www.youtube.com/embed/<%= video.youtube_id %>?start=<%= seek - 5 %>&enablejsapi=1"
          frameborder="0"></iframe>
<% else %>
  <iframe id="player"
          type="text/html"
          width="640"
          height="390"
          src="https://www.youtube.com/embed/<%= video.youtube_id %>?enablejsapi=1"
          frameborder="0"></iframe>
<% end %>
